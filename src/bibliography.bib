journal = {Urology},
month = {nov},
number = {5},
pages = {931--935},
pmid = {18068450},
publisher = {Elsevier},
title = {{Contemporary Evaluation of the D'Amico Risk Classification of Prostate Cancer}},
url = {http://www.sciencedirect.com/science/article/pii/S0090429507020973?via%3Dihub},
volume = {70},
year = {2007}
}
