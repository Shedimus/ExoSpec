publisher = {NIH Public Access},
title = {{TMPRSS2-ERG gene fusion is not associated with outcome in patients treated by prostatectomy}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19190343 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC3676271},
volume = {69},
year = {2009}
