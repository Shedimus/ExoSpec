journal = {Future oncology (London, England)},
keywords = {Humans,Male,Practice Guidelines as Topic,Prostatic Neoplasms,Prostatic Neoplasms: classification,Prostatic Neoplasms: pathology,Risk Assessment,Risk Assessment: methods},
month = {dec},
