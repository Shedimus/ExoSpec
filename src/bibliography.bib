eprint = {NIHMS150003},
file = {:Users/shea/Documents/PhD/Literature & Books/Mendeley/Cooperberg, Hilton, Carroll - 2011 - The CAPRA-S score A straightforward tool for improved prediction of outcomes after radical prostate.pdf:pdf},
