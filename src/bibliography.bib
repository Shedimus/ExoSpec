isbn = {0008-543X (Print)},
issn = {0008543X},
keywords = {Animal models,Cell culture,Diet,Environment,Epidemiology,Genetics,Pathology,Prostate cancer,Prostatic neoplasms,Risk},
month = {nov},
number = {10 SUPPL.},
pages = {2371--2490},
pmid = {15495199},
publisher = {Wiley Subscription Services, Inc., A Wiley Company},
title = {{Human prostate cancer risk factors}},
