eprint = {NIHMS150003},
isbn = {0270-4137},
issn = {10970045},
journal = {Prostate},
keywords = {Peripheral zone prostate cancer,Prostate neoplasms,Prostate specific antigen,Prostatectomy,Transition zone prostate cancer},
month = {feb},
number = {2},
pages = {183--190},
pmid = {25327466},
title = {{Biologic differences between peripheral and transition zone prostate cancer}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/25327466 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4270836 http://doi.wiley.com/10.1002/pros.22903},
