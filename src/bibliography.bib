title = {{EAU-ESTRO-SIOG Guidelines on Prostate Cancer. Part 1: Screening, Diagnosis, and Local Treatment with Curative Intent}},
url = {http://dx.doi.org/10.1016/j.eururo.2016.08.003},
volume = {71},
