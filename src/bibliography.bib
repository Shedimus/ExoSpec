doi = {10.1016/j.csbj.2014.11.005},
eprint = {9781591404590},
file = {:Users/shea/Documents/PhD/Literature & Books/Mendeley/Kourou et al. - 2015 - Machine learning applications in cancer prognosis and prediction.pdf:pdf},
isbn = {2001-0370 (Electronic)\r2001-0370 (Linking)},
issn = {20010370},
keywords = {Cancer recurrence,Cancer survival,Cancer susceptibility,Machine learning,Predictive models},
