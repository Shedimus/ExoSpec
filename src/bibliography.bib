title = {{Tumor-derived exosomes in cancer progression and treatment failure}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/26452221 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4741921 http://www.oncotarget.com/fulltext/6022},
