title = {{Molecular genetics of prostate cancer: new prospects for old challenges.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/20844012 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2939361 http://www.ncbi.nlm.nih.gov/pubmed/20844012%5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC2939361},
volume = {24},
